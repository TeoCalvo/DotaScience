pipeline {
    agent any

    stages{

        stage("History Matches"){
            steps{
                sh "docker exec dota_con python dotaScience/hook/get_match_history.py"
            }
        }

        stage("Match Details"){
            steps{
                sh "docker exec dota_con python dotaScience/hook/get_match_details.py"
            }
        }
    }
}